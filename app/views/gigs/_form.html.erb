<%= semantic_form_for @gig, html: { class: 'entry-container admin gig' } do |f| %>
	<%= f.input :name %>
	<%= f.input :city, wrapper_html: { class: 'city' } %>
	<%= f.input :state, wrapper_html: { class: 'state' } %>
	<%= f.input :act %>
	<%= f.input :description %>
	<%= f.input :link, label: "Link for more info" %>
	<%= f.submit %>
<% end %>
